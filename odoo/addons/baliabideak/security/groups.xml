<odoo>
    <data>
        <record model="ir.module.category" id="module_category_tresnak">
            <field name="name">Tresnak kudeatu</field>
            <field name="description">Baliabideak kudeatzeko</field>
            <field name="sequence">10</field>
        </record>

        <record id="group_tresnak_reader" model="res.groups">
            <field name="name">Irakurlea</field>
            <field name="category_id" ref="module_category_gym"/>
        </record>

        <record id="group_tresnak_employee" model="res.groups">
            <field name="name">Langilea</field>
            <field name="category_id" ref="module_category_tresnak"/>
            <field name="implied_ids" eval="[(3, ref(group_tresnak_reader'))]"/>
            <field name="users" eval="[(3, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>

        <record id="group_tresnak_admin" model="res.groups">
            <field name="name">Administratzailea</field>
            <field name="category_id" ref="module_category_gym"/>
            <field name="implied_ids" eval="[(4, ref('group_tresnak_reader'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>