<odoo>
    <record id="view_gym_bazkideak_tree" model="ir.ui.view">
        <field name="name">gym.bazkideak.tree</field>
        <field name="model">gym.bazkideak</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="view_gym_bazkideak_form" model="ir.ui.view">
        <field name="name">gym.bazkideak.form</field>
        <field name="model">gym.bazkideak</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="action_gym_bazkideak" model="ir.actions.act_window">
        <field name="name">Bazkideak</field>
        <field name="res_model">gym.bazkideak</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="menu_gym_root" 
              name="Gym Kudeaketa" 
              sequence="10"/>
    <menuitem id="menu_gym_bazkideak" 
              name="Bazkideak" 
              parent="menu_gym_root" 
              action="action_gym_bazkideak"/>
</odoo>