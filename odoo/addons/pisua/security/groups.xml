<odoo>
    <data noupdate="1">
        <record id="module_category_pisua" model="ir.module.category">
            <param name="name">Gesti√≥n de Pisos</param>
            <param name="sequence">10</param>
        </record>

        <record id="group_pisua_user" model="res.groups">
            <field name="name">Miembro</field>
            <field name="category_id" ref="module_category_pisua"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="group_pisua_manager" model="res.groups">
            <field name="name">Coordinador</field>
            <field name="category_id" ref="module_category_pisua"/>
            <field name="implied_ids" eval="[(4, ref('group_pisua_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>